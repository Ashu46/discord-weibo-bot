{
  "name": "Discord Weibo Bot",
  "description": "Post Weibo posts to Discord through Webhook",
  "website": "https://github.com/theandychung/discord-weibo-bot",
  "repository": "https://github.com/theandychung/discord-weibo-bot",
  "logo": "https://i.imgur.com/bKRd34R.png",
  "keywords": [
      "discord",
      "weibo",
      "webhook"
  ],
  "env": {
    "1.WEBHOOK_URL": {
      "description": "Discord webhook URL https://support.discordapp.com/hc/en-us/articles/228383668-Intro-to-Webhooks"
    },
    "2.APP_KEY": {
      "description": "Weibo app key"
    },
    "3.APP_SECRET": {
      "description":  "Weibo app secret"
    },
    "4.REDIRECT_URL": {
      "description": "Redirect URL",
      "value": "http://api.weibo.com/oauth2/default.html"
    },
    "5.Weibo ID":{
      "description": "The ID of Weibo user(s) you want to track with this bot, use (,) to split. Ex: 123, 456, 123"
    },
    "6.USERNAME": {
      "description": "Your Weibo username",
      "required": false
    },
    "7.PASSWORDS": {
      "description": "Your Weibo passwords",
      "required": false
    }
  },
  "buildpacks": [
    {
      "url": "https://github.com/dmathieu/heroku-buildpack-submodules"
    }
  ]
}